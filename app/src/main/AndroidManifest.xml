<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="feicui.com.app">

    <!-- 外部存储器的写入权限 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--读取通讯录的权限-->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <!--写入通讯录的权限-->
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="feicui.com.hk.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="feicui.com.hk.ViewAnim" />
        <activity android:name="feicui.com.hk.XmlanimActivity" />
        <activity android:name="feicui.com.hk.datastorage.CacheFile" />
        <activity android:name="feicui.com.hk.datastorage.ExternalStorage" />
        <activity android:name="feicui.com.hk.datastorage.InternalStorage" />
        <activity android:name="feicui.com.hk.datastorage.SharePre" />
        <activity android:name="feicui.com.hk.drawableanim.DrawableAnim" />
        <activity android:name="feicui.com.transition.Transition" />
        <activity android:name="feicui.com.hk.datastorage.ContentProviderActivity"/>
    </application>

</manifest>